<h2 class="title">Create a new user</h2>

<form method="post" action="/piece" id="form_create_piece" class="form">
    <label for="marque">Piece Marque</label>
    <input id="marque" type="text" value="foo" name="pieceMarque" required>

    <label for="model">Model</label>
    <input id="model" type="text" value="foo" name="model" required>
    
    <label for="genre">Genre</label>
    <input id="genre" type="text" value="bar" name="genre" required>

    <label for="year">Year</label>
    <input id="year" type="number" value="1998" name="year" required>

    <label for="descriptif">Descriptif</label>
    <input id="descriptif" type="number" value="38" name="descriptif" required>

    <label for="image">Image</label>
    <input id="image" type="file" value="image" name="imag"></input>

    <button id="btn_create_user" class="btn">Create</button>
</form>